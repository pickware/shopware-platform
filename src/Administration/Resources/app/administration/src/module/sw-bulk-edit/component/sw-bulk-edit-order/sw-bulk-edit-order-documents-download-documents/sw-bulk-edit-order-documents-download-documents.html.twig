{% block sw_bulk_edit_order_documents_download_documents %}
<div
    v-if="documentTypes && documentTypes.total > 0"
    class="sw-bulk-edit-order-documents-download-documents"
>
    {% block sw_bulk_edit_order_documents_download_documents_content %}
    <mt-checkbox
        v-for="documentType in documentTypes"
        :key="documentType.id"
        v-model:checked="documentType.selected"
        :label="documentType.translated.name"
        class="sw-bulk-edit-order-documents-download-documents__checkbox"
        :class="`is--${documentType.technicalName}`"
    />
    {% endblock %}
</div>
{% endblock %}
