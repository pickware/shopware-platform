{% block sw_url_field %}
<mt-url-field
    v-bind="$attrs"
    v-model="realValue"
>
    <template
        v-for="(index, name) in getSlots()"
        #[name]="data"
    >
        <slot
            :name="name"
            v-bind="data || {}"
        >
        </slot>
    </template>
</mt-url-field>
{% endblock %}

{% block sw_url_field_deprecated %}
{% endblock %}
