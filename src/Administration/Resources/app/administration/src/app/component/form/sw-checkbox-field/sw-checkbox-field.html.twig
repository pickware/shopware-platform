{% block sw_checkbox_field %}
<mt-checkbox
    v-bind="$attrs"
    :checked="compatValue"
    @update:checked="handleUpdateChecked"
>
    <template
        v-for="(index, name) in getSlots()"
        #[name]="data"
    >
        <slot
            :name="name"
            v-bind="data"
        >
        </slot>
    </template>
    <slot></slot>
</mt-checkbox>
{% endblock %}
